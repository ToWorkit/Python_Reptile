    "# match\n",
    "content = 'Hello 80 World'\n",
    "# 查看长度\n",
    "print(len(content))\n",
    "result = re.match('^Hello\\s\\d?.*World$', content)\n",
    "print(result)\n",
    "# 查看结果\n",
    "print(result.group())\n",
    "# 查看匹配范围\n",
    "print(result.span())"
